#!/bin/bash
#$ -S /bin/bash
#$ -cwd
#$ -j y
#$ -pe threaded 24
#$ -o nam.out

RUNID=1

EXEC=$PBSAMHOME/working_code/PBSAM_all/bin/bdnam_10
MOLDIR=$PBSAMHOME/working_code/PBSAM_all/test/1BRS
THREAD=24
SALT_CONC=0.05
NTRAJ=500
DOCKDIST=3.5
RUNNAME=1BRS_i"$SALT_CONC"_d"$DOCKDIST"_r"$RUNID"
STATFILE=stat_"$RUNNAME".dat
CONTACTFILE=$MOLDIR/PREP_findContacts/contactdef_c2.dat

PQR1=$MOLDIR/config/1BRS_chainA_p1.5d3_s5i1.pqr
IMAT1=$MOLDIR/DATA_s5i1/makeIMat/IMAT_P10_250k/chainA
EXPPATH1=$MOLDIR/DATA_s5i1/DATA_SELFPOL/SOLEXP_p10/chainA
EXPNAME1=1BRS_A_s5i1_i0.05_p10.0

PQR2=$MOLDIR/config/1BRS_chainD_p1.5d3_s5i1.pqr
IMAT2=$MOLDIR/DATA_s5i1/makeIMat/IMAT_P10_250k/chainD
EXPPATH2=$MOLDIR/DATA_s5i1/DATA_SELFPOL/SOLEXP_p10/chainD
EXPNAME2=1BRS_D_s5i1_i0.05_p10.0

$EXEC $THREAD $SALT_CONC $NTRAJ $DOCKDIST $STATFILE $RUNNAME $CONTACTFILE $PQR1 $IMAT1 $EXPPATH1 $EXPNAME1 $PQR2 $IMAT2 $EXPPATH2 $EXPNAME2
